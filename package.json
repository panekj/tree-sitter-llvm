{
  "name": "tree-sitter-llvm",
  "version": "1.0.0",
  "description": "Tree sitter parser for LLVM",
  "main": "bindings/node",
  "types": "bindings/node",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "install": "node-gyp-build",
    "prebuildify": "prebuildify --napi --strip"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benwilliamgraham/tree-sitter-llvm.git"
  },
  "author": "Benjamin Graham",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/benwilliamgraham/tree-sitter-llvm/issues"
  },
  "homepage": "https://github.com/benwilliamgraham/tree-sitter-llvm#readme",
  "dependencies": {
    "node-addon-api": "^7.1.0",
    "node-gyp-build": "^4.8.0"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.0"
  },
  "peerDependenciesMeta": {
    "tree_sitter": {
      "optional": true
    }
  },
  "devDependencies": {
    "prettier": "2.4.1",
    "tree-sitter-cli": "^0.20.0",
    "prebuildify": "^6.0.0"
  },
  "tree-sitter": [
    {
      "scope": "source.llvm",
      "file-types": [
        "llvm",
        "ll"
      ]
    }
  ],
  "directories": {
    "example": "example"
  },
  "keywords": [
    "parser"
  ],
  "files": [
    "grammar.js",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**"
  ]
}
